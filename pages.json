{
	"easycom": {
			"autoscan": true,
			// 注意一定要放在custom里，否则无效，https://ask.dcloud.net.cn/question/131175
			"custom": {
				"^u--(.*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue",
				"^up-(.*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue",
		    "^u-([^-].*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue"
			}
		},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "登录",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/my/index",
			"style": {
				"navigationBarTitleText": "my"
			}
		},
		{
			"path": "pages/business/business",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/projectDetails/projectDetails",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		}
	],
	"tabBar": {
		"color": "#000000",
		"selectedColor": "#2D6AF6",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
				"pagePath": "pages/index/index",
				"iconPath": "static/icons/AI学习-默认@2x.png",
				"selectedIconPath": "static/icons/AI学习-选中@2x.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/business/business",
				"iconPath": "static/icons/业务-默认@2x.png",
				"selectedIconPath": "static/icons/业务-选中@2x.png",
				"text": "业务"
			}, {
				"pagePath": "pages/my/index",
				"iconPath": "static/icons/我的-默认@2x.png",
				"selectedIconPath": "static/icons/我的-选中@2x.png",
				"text": "我的"
			}
		]
	},
	"globalStyle": {
		"navigationStyle": "custom",
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}